<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/9" xmlns:fx="http://javafx.com/fxml/1" fx:controller="UIUX.Controllers.ChallengesPageController">
   <children>
      <VBox prefHeight="720.0" prefWidth="1280.0">
         <children>
            <HBox alignment="CENTER_LEFT" prefHeight="105.0" prefWidth="1280.0">
               <children>
                  <Button alignment="CENTER" mnemonicParsing="false" onAction="#backToMenu" prefHeight="34.0" prefWidth="76.0" text="Back">
                     <HBox.margin>
                        <Insets left="40.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
            <ScrollPane prefHeight="423.0" prefWidth="1200.0">
               <content>
                  <VBox fx:id="challengesVBox" prefWidth="1270.0" />
               </content>
               <VBox.margin>
                  <Insets left="40.0" right="40.0" />
               </VBox.margin>
            </ScrollPane>
            <HBox alignment="CENTER_RIGHT" prefHeight="100.0" prefWidth="200.0" spacing="45.0">
               <children>
                  <Button mnemonicParsing="false" onAction="#createFilter" text="Filter" />
                  <Button mnemonicParsing="false" onAction="#createNewChallenge" text="New Challenge">
                     <HBox.margin>
                        <Insets right="40.0" />
                     </HBox.margin>
                  </Button>
               </children></HBox>
         </children>
      </VBox>
   </children>
</AnchorPane>
