<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="720.0" prefWidth="1280.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ClientAPP.Controllers.ProfilePageController">
   <children>
      <Label layoutX="168.0" layoutY="301.0" text="Username">
         <font>
            <Font size="22.0" />
         </font>
      </Label>
      <TextField fx:id="usernameField" editable="false" layoutX="314.0" layoutY="293.0" promptText="Username">
         <font>
            <Font size="22.0" />
         </font>
      </TextField>
      <Label layoutX="168.0" layoutY="378.0" text="Password">
         <font>
            <Font size="22.0" />
         </font>
      </Label>
      <PasswordField fx:id="passwordField" editable="false" layoutX="314.0" layoutY="371.0" prefHeight="42.0" prefWidth="288.0" promptText="Password">
         <font>
            <Font size="22.0" />
         </font>
      </PasswordField>
      <Label layoutX="168.0" layoutY="453.0" text="Name">
         <font>
            <Font size="22.0" />
         </font>
      </Label>
      <TextField fx:id="nameField" editable="false" layoutX="314.0" layoutY="445.0" promptText="Name">
         <font>
            <Font size="22.0" />
         </font>
      </TextField>
      <Label layoutX="168.0" layoutY="529.0" text="Rating">
         <font>
            <Font size="22.0" />
         </font>
      </Label>
      <TextField fx:id="ratingField" editable="false" layoutX="314.0" layoutY="521.0" promptText="Rating">
         <font>
            <Font size="22.0" />
         </font>
      </TextField>
      <ImageView fx:id="profilePicture" fitHeight="285.0" fitWidth="288.0" layoutX="790.0" layoutY="284.0" pickOnBounds="true" preserveRatio="true">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </ImageView>
      <Label layoutX="478.0" layoutY="54.0" text="Profile">
         <font>
            <Font name="System Bold" size="85.0" />
         </font>
      </Label>
      <Button fx:id="usernameChangeButton" layoutX="618.0" layoutY="293.0" mnemonicParsing="false" onAction="#tryChangingUsername" prefHeight="42.0" prefWidth="80.0" text="Change">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button fx:id="passwordChangeButton" layoutX="618.0" layoutY="371.0" mnemonicParsing="false" prefHeight="42.0" prefWidth="80.0" text="Change">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button fx:id="nameChangeButton" layoutX="618.0" layoutY="445.0" mnemonicParsing="false" prefHeight="42.0" prefWidth="80.0" text="Change">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button fx:id="ratingChangeButton" disable="true" layoutX="618.0" layoutY="521.0" mnemonicParsing="false" prefHeight="42.0" prefWidth="80.0" text="Change">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
      <Button fx:id="backButton" layoutX="80.0" layoutY="84.0" mnemonicParsing="false" onAction="#goToMainMenu" prefHeight="42.0" prefWidth="96.0" text="Back">
         <font>
            <Font name="System Italic" size="17.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
